{
  "manifest_version": 3,
  "name": "Groww",
  "version": "1.0.0",
  "description": "Add features to Groww stocks page",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": ["stocks.js"],
      "matches": ["https://groww.in/stocks/*"]
    }
  ],
  "icons": {
    "16": "app_logo.png",
    "48": "app_logo.png", 
    "128": "app_logo.png" 
  }
}